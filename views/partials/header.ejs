<style>
  html,
  body {
    margin: 0;
    /* overflow: hidden !important; */
    height: 100%;
  }

  .preloader-container {
    height: 100vh;
    display: flex;
    overflow: hidden !important;
    position: absolute;
    width: 100%;
    background-color: aliceblue;
    z-index: 1111111;
    justify-content: center;
    align-items: center;
    flex-direction: column;
  }

  .preloader-circle,
  .preloader-circle::before {
    content: " ";
    margin: 15px;
    z-index: 1111;
    width: 15px;
    height: 15px;
    border-radius: 50%;
    margin: 0 4px;
    transition: all 0.3s;
    background-color: #a9a8a8;
  }

  .preloader-circle::before {
    animation: mymove .5s infinite;
    position: absolute;
    background-color: #ccc
  }

  @keyframes mymove {
    50% {
      transform: scale(2);
      opacity: 0
    }

    100% {
      transform: scale(2);
      opacity: 0
    }
  }
</style>

